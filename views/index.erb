<h2>Inventory</h2>


<table>
  <tr>
    <th>Product</th>

    <th>Stock Quantity</th>

    <th>Supplier</th>

    <th>To reorder, call:</th>

    <th>Comments</th>
  </tr>
  <% for product in @products %>
  <tr>
    <td><%= product.product %></td>

    <td><%= product.stock_quantity %></td>

    <td><%= product.supplier.name %></td>

    <td><%= product.supplier.telephone_number %></td>
    <% end %>

  </tr>
  
</table>
